        <!ELEMENT player (id, name, account)>
        <!ELEMENT account (id, email, password, age, balance, credits, transactions, bets)>
        <!ELEMENT email (#PCDATA)>
        <!ELEMENT password (#PCDATA)>
        <!ELEMENT age (#PCDATA)>
        <!ELEMENT balance (#PCDATA)>
        <!ELEMENT credits (#PCDATA)>
